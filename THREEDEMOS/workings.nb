           RowBox[{"h", "/", "2"}]}]], "]"}], "-", 
        RowBox[{"ArcTan", "[", 
         FractionBox[
          RowBox[{"d", " ", 
           RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], 
          RowBox[{
           RowBox[{"d", " ", 
            RowBox[{"Cos", "[", "\[Alpha]", "]"}]}], "+", 
           RowBox[{"h", "/", "2"}]}]], "]"}]}], "\[Equal]", "\[Theta]2"}], 
      ",", "d"}], "]"}], "//", "FullSimplify"}], "//", "Last"}], "//", 
  "Last"}]}], "Input",
