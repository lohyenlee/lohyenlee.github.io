Cell[TextData[{
 "We know ",
 StyleBox["h",
  FontSlant->"Italic"],
 ", \[Theta]",
 ", \[Alpha].  We wish find ",
 StyleBox["Y",
  FontSlant->"Italic"],
 " and ",
 StyleBox["Z",
  FontSlant->"Italic"],
 ".  ",
 "Eliminating ",
 StyleBox["Z",
  FontSlant->"Italic"],
 " gives ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Alpha]", "+", "\[Theta]"}], ")"}], "tan", " ", 
     RowBox[{"(", 
      RowBox[{"\[Alpha]", "+", "\[Theta]"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"Y", "+", 
       FractionBox["h", "2"]}], ")"}], "tan", " ", 
     RowBox[{"(", 
      RowBox[{"\[Alpha]", "-", "\[Theta]"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"d3b27f6a-0a17-457b-ae08-db6c209da4a5"],
 ".  Rearranging for ",
 StyleBox["Y",
  FontSlant->"Italic"],
 " gives",
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", "=", 
    RowBox[{
     RowBox[{
      FractionBox["h", "2"], 
      FractionBox[
       RowBox[{
        RowBox[{"tan", " ", 
         RowBox[{"(", 
          RowBox[{"\[Alpha]", "+", "\[Theta]"}], ")"}]}], "+", 
        RowBox[{"tan", "(", 
         RowBox[{"\[Alpha]", "-", "\[Theta]"}], ")"}]}], 
       RowBox[{
        RowBox[{"tan", " ", 
         RowBox[{"(", 
          RowBox[{"\[Alpha]", "+", "\[Theta]"}], ")"}]}], "-", 
        RowBox[{"tan", " ", 
         RowBox[{"(", 
          RowBox[{"\[Alpha]", "-", "\[Theta]"}], ")"}]}]}]]}], "=", 
     RowBox[{
      FractionBox["h", "2"], 
      FractionBox[
       RowBox[{"sin", " ", "2", "\[InvisibleSpace]", "\[Alpha]"}], 
       RowBox[{"sin", " ", "2", "\[InvisibleSpace]", "\[Theta]"}]]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "584cb98c-ec70-47cd-873a-2a0c898312aa"],
 ".\nThen\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"Z", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Y", "-", 
        FractionBox["h", "2"]}], ")"}], 
      RowBox[{"tan", "(", 
       RowBox[{"\[Alpha]", "+", "\[Theta]"}], ")"}]}], "=", 
     RowBox[{
      FractionBox["h", "2"], 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{
          RowBox[{"tan", " ", 
           RowBox[{"(", 
            RowBox[{"\[Theta]", "+", "\[Alpha]"}], ")"}]}], "+", 
          RowBox[{"tan", "(", 
           RowBox[{"\[Theta]", "-", "\[Alpha]"}], ")"}]}], 
         RowBox[{
          RowBox[{"tan", " ", 
           RowBox[{"(", 
            RowBox[{"\[Theta]", "+", "\[Alpha]"}], ")"}]}], "-", 
          RowBox[{"tan", " ", 
           RowBox[{"(", 
            RowBox[{"\[Theta]", "-", "\[Alpha]"}], ")"}]}]}]], "-", "1"}], 
       ")"}], 
      RowBox[{"tan", "(", 
       RowBox[{"\[Alpha]", "+", "\[Theta]"}], ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "70f5ce71-c897-4eab-a1b7-f538970763f1"],
 ".\nThe camera distance from the origin is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "=", 
    SqrtBox[
     RowBox[{
      SuperscriptBox["Y", "2"], "+", 
      SuperscriptBox["Z", "2"]}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "25bdf756-997d-4c89-82ad-5a9ea7d3afbf"],
 ", i.e.,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"d", "=", 
    RowBox[{
     FractionBox["h", "2"], 
     FractionBox[
      RowBox[{
       SqrtBox[
        RowBox[{"3", "+", 
         RowBox[{"Cos", "[", 
          RowBox[{"4", " ", "\[Alpha]"}], "]"}], "-", 
         RowBox[{"4", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Alpha]"}], "]"}], " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}]], " ", 
       RowBox[{"Csc", "[", 
        RowBox[{"2", " ", "\[Alpha]"}], "]"}]}], 
      SqrtBox["2"]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5a114769-b114-4f02-9cdb-896c1c299f25"],
 ".\nThe inclination of the line from the camera to the origin is \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "true"], "=", 
    RowBox[{"arctan", " ", 
     FractionBox["Z", "Y"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fc7bb3e8-7f11-4abf-a14b-5a3d26730c30"],
 "."
}], "Text",
